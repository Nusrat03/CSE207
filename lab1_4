#include<stdio.h>
void swap(float *x, float *y, float *z)
{
  int temp;
  temp=*x;
  *x=*y;
  *y=*z;
  *z=temp;
}
int main(){
  float a, b, c;
  printf("Enter the numbers : \n");
  scanf("%f %f %f", &a, &b, &c);
  swap(&a, &b, &c);
  printf("The values after swapping : %f %f %f", a, b, c);

  return 0;
}
